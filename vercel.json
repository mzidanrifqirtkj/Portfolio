{
  "build": {
    "env": {
      "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES": "true"
    }
  },
  "functions": {
    "src/api/**/*.js": {
      "memory": 1024
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
